{
  "name": "react-ssr-boilerplate",
  "version": "2.12.1",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "start-prod": "node server/index.js",
    "precommit": "lint-staged",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,json,css,md}\"",
    "docker:build": "docker build --rm -t cullenjett/react-ssr-boilerplate .",
    "docker:start": "docker run --rm -it -p 3000:3000 cullenjett/react-ssr-boilerplate",
    "docker": "npm run docker:build && npm run docker:start"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ],
    "*.{js,json,css,md}": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.28",
    "autoprefixer": "^9.4.4",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "bluebird": "^3.5.3",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.4.2",
    "compression": "^1.7.3",
    "core-js": "^2.6.2",
    "css-loader": "^2.1.0",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "error-overlay-webpack-plugin": "^0.1.6",
    "eslint": "^5.12.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "form-data": "^2.3.3",
    "fs-extra": "^7.0.1",
    "helmet": "^3.15.0",
    "history": "^4.7.2",
    "husky": "^1.3.1",
    "immutability-helper": "^2.9.0",
    "immutable": "^4.0.0-rc.12",
    "import-glob-loader": "^1.1.0",
    "jest": "^23.6.0",
    "keycloak-js": "^4.8.3",
    "lint-staged": "^8.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "lodash.debounce": "^4.0.8",
    "lodash.flow": "^3.5.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.5.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "rc-color-picker": "^1.2.6",
    "react": "^16.7.0",
    "react-dd-menu": "^2.0.2",
    "react-dev-utils": "^7.0.1",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "^16.7.0",
    "react-dropzone": "^8.0.3",
    "react-grid-layout": "^0.16.6",
    "react-helmet": "^5.2.0",
    "react-iframe": "^1.3.3",
    "react-infinite-scroller": "^1.2.4",
    "react-loadable": "^5.5.0",
    "react-masonry-component": "^6.2.1",
    "react-paginate": "^6.2.1",
    "react-pdf": "^4.0.2",
    "react-player": "^1.8.0",
    "react-quill": "^1.3.3",
    "react-redux": "^6.0.0",
    "react-responsive-modal": "^3.5.1",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "react-s-alert": "^1.4.1",
    "react-select": "^2.3.0",
    "react-simple-dropdown": "^3.2.3",
    "react-slick": "^0.23.2",
    "react-spinners": "^0.5.1",
    "react-swipeable-views": "^0.13.1",
    "react-test-renderer": "^16.7.0",
    "react-textarea-autosize": "^7.1.0",
    "react-transition-group": "^2.5.3",
    "react-widgets": "^4.4.9",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.0",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "slate": "^0.44.9",
    "slate-html-serializer": "^0.7.33",
    "slate-react": "^0.21.15",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "typeface-roboto": "0.0.54",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/browserMocks.js",
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
